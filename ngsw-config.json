{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "GitHubAPI",
      "urls": [
        "https://api.github.com/**",
        "https://avatars*.githubusercontent.com/u/*"
      ],
      "version": 3,
      "cacheConfig": {
        "maxSize": 500,
        "maxAge": "2d",
        "strategy": "freshness"
      }
    },
    {
      "name": "GoogleAssets",
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "30d",
        "strategy": "performance"
      }
    }
  ]
}
